<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".UtilityMainActivity">

    <ImageButton
        android:layout_width="241dp"
        android:layout_height="170dp"
        android:id="@+id/haulerButton"
        android:background="@drawable/cushman_pro"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="241dp"
        android:layout_height="170dp"
        android:id="@+id/hauler4Button"
        android:background="@drawable/cushman_hauler"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="241dp"
        android:layout_height="170dp"
        android:id="@+id/haulerXButton"
        android:background="@drawable/cushman_pro"
        android:layout_above="@+id/textView8"
        android:layout_alignLeft="@+id/hauler4Button"
        android:layout_alignStart="@+id/hauler4Button" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Hauler 4x4"
        android:id="@+id/textView7"
        android:layout_below="@+id/hauler4Button"
        android:layout_alignRight="@+id/textView9"
        android:layout_alignEnd="@+id/textView9" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Hauler X"
        android:id="@+id/textView8"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/textView7"
        android:layout_alignEnd="@+id/textView7" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Hauler"
        android:id="@+id/textView9"
        android:layout_marginRight="83dp"
        android:layout_marginEnd="83dp"
        android:layout_below="@+id/haulerButton"
        android:layout_alignRight="@+id/haulerButton"
        android:layout_alignEnd="@+id/haulerButton" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView2"
        android:background="@drawable/icon"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ViewFlipper
        android:layout_width="475dp"
        android:layout_height="319dp"
        android:id="@+id/viewFlipper11"
        android:touchscreenBlocksFocus="false"
        android:animateFirstView="false"
        android:forceHasOverlappingRendering="false"
        android:flipInterval="3000"
        android:autoStart="true"
        android:layout_below="@+id/haulerButton"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <ImageView
            android:layout_width="475dp"
            android:layout_height="319dp"
            android:id="@+id/imageView25"
            android:background="@drawable/hauler_800_slideshow_1"
            android:layout_alignTop="@+id/browseCushman"
            android:layout_centerHorizontal="true" />

        <ImageView
            android:layout_width="475dp"
            android:layout_height="319dp"
            android:id="@+id/imageView24"
            android:background="@drawable/hauler_800_slideshow_2"
            android:layout_above="@+id/commercialButton"
            android:layout_toRightOf="@+id/commercialButton"
            android:layout_toEndOf="@+id/commercialButton" />
    </ViewFlipper>
</RelativeLayout>
